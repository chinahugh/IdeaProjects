server:
  port: 8080
debug: true
spring:
    datasource:
        name: mysql_test
        type: com.alibaba.druid.pool.DruidDataSource
        driver-class-name: com.mysql.jdbc.Driver
          #基本属性
        url: jdbc:mysql://localhost:3306/springboot2
        username: root
        password: root
        #配置初始化大小/最小/最大
        druid:
         #监控统计拦截的filters
          filters: stat
          initial-size: 1
          min-idle: 1
          max-active: 10
          #获取连接等待超时时间
          max-wait: 60000
          #间隔多久进行一次检测，检测需要关闭的空闲连接
          time-between-eviction-runs-millis: 60000
          #一个连接在池中最小生存的时间
          min-evictable-idle-time-millis: 300000
          validation-query: SELECT 'x'
          test-while-idle: true
          test-on-borrow: false
          test-on-return: false
          #打开PSCache，并指定每个连接上PSCache的大小。oracle设为true，mysql设为false。分库分表较多推荐设置为false
          pool-prepared-statements: false
          max-pool-prepared-statement-per-connection-size: 20
    aop:
        auto: true
        proxy-target-class: true
    thymeleaf:
      cache: false
      mode: LEGACYHTML5
    #rides
    redis:
      host: localhost
      port: 6379
      jedis:
        pool:
        # 连接池中的最大空闲连接
          max-idle: 8
          # 连接池中的最小空闲连接
          min-idle: 0
          # 连接池最大连接数（使用负值表示没有限制）
          max-active: 8
          # 连接池最大阻塞等待时间（使用负值表示没有限制）
          max-wait: -1
      # 连接超时时间（毫秒）
      timeout: 0
      password:
    mvc:
      favicon:
        enabled: false
#mybatis
mybatis:
  mapper-locations: classpath:/mapper/test/*.xml
  type-aliases-package: com.demo.test

#pagehelper
pagehelper:
    helperDialect: mysql
    reasonable: true
    supportMethodsArguments: true
    params: count=countSql
mapper:
  not-empty: false
  identity: MYSQL


